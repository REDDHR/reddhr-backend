{
	"name": "reddhr-backend",
	"version": "1.0.0",
	"description": "Reddhr backend server for the Reddhr app",
	"main": "index.ts",
	"type": "module",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start:dev": "npx nodemon",
		"prepare": "husky install",
		"precommit": "lint-staged --debug --verbose",
		"lint:fix": "eslint --fix",
		"prettier:fix": "prettier --write",
		"build": "rimraf ./build && tsc",
		"start:prod": "npm run build && npm run compile && node build/index.js",
		"compile:dev": "babel . -d dist -s --source-maps --copy-files"
	},
	"lint-staged": {
		"*.{js,ts}": [
			"npm run lint:fix",
			"npm run prettier:fix"
		]
	},
